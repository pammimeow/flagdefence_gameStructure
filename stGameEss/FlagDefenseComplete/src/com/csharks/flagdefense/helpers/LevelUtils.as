		{
		}
		/**
		 * Create the collision array from level data 
		 * 
		 */
		public static function initialise():void{
			rows=groundArray.length;
			cols=groundArray[0].length;
			for(var i:int=0;i<rows;i++){
				collisionArray[i]=new Array();
				for(var j:int=0;j<cols;j++){
					if(overlayArray[i][j]!="*"||groundArray[i][j]=="tile4.png"){
						collisionArray[i].push(1);
					}else{
						collisionArray[i].push(0);
					}
				}
			}
		}
		
